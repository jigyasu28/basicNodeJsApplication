<!DOCTYPE html>
<html>
<head>
  <title>Mobile Phones</title>
  <style>
    body {
      font-family: Arial, sans-serif;
      margin: 0;
      padding: 0;
    }
    p{
      text-align: center;
    }
    h1 {
      text-align: center;
      margin-top: 50px;
    }
    
    table {
      width: 100%;
      border-collapse: collapse;
      margin-top: 30px;
    }
    
    th {
      background-color: #009688;
      color: #fff;
      padding: 10px;
    }
    
    td {
      border: 1px solid #ddd;
      padding: 10px;
    }
    
    tr:nth-child(even) {
      background-color: #f2f2f2;
    }
    
    tr:hover {
      background-color: #ddd;
    }
    
    .center {
      text-align: center;
    }
  </style>
</head>
<body>
  <h1>Mobile Phones</h1>
  <table>
    <thead>
      <tr>
        <th>Name</th>
        <th>Brand</th>
        <th>Price</th>
        <th>Rating</th>
        <th>Action</th>
      </tr>
    </thead>
    <tbody>
      <tr>
        <td>iPhone 12 Pro</td>
        <td>Apple</td>
        <td>$999</td>
        <td>4.8</td>
        <td class="center"><button>Add to Cart</button></td>
      </tr>
      <tr>
        <td>Galaxy S21</td>
        <td>Samsung</td>
        <td>$799</td>
        <td>4.5</td>
        <td class="center"><button>Add to Cart</button></td>
      </tr>
      <tr>
        <td>Pixel 5</td>
        <td>Google</td>
        <td>$699</td>
        <td>4.7</td>
        <td class="center"><button>Add to Cart</button></td>
      </tr>
      <!-- Add more mobiles here -->
    </tbody>
  </table>
  <div class="Others">
    <p><a href="contact">Contact Us</a></p>
    <p><a href="logout">Logout</a></p>
  </div>
</body>
</html>
